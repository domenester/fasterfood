{
  "name": "faster-food",
  "version": "0.1.0",
  "main": "./index.js",
  "description": "A managment app",
  "private": false,
  "author": "Diogo Domene",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/domenester/fasterfood.git"
  },
  "scripts": {
    "postinstall": "install-app-deps && bower install --config.interactive=false",
    "start": "npm install && electron ./app",
    "dev": "gulp build && electron ./",
    "pack": "build --dir",
    "dist": "build"
  },
  "build": {
    "appId": "yourappid",
    "win": {
      "target": "portable",
      "icon": "build/skull.ico"
    },
    "electronCompile": false
  },
  "dependencies": {
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.13.1",
    "bunyan": "^1.8.12",
    "bunyan-prettystream": "^0.1.3",
    "chalk": "^2.3.0",
    "compression": "^1.5.0",
    "connect-flash": "~0.1.1",
    "consolidate": "^0.14.5",
    "cookie-parser": "^1.3.2",
    "crypto": "^1.0.1",
    "electron-compile": "^6.4.2",
    "electron-log": "^2.2.11",
    "express": "^4.13.1",
    "express-session": "^1.15.6",
    "glob": "^5.0.13",
    "lodash": "^3.10.0",
    "method-override": "^2.3.3",
    "mongoose": "^4.11.12",
    "nodemailer": "^4.4.0",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "swig": "^1.4.2",
    "tingodb": "^0.5.1"
  },
  "devDependencies": {
    "electron": "^1.7.8",
    "electron-builder": "^19.29.1",
    "eslint": "^4.12.0",
    "gulp": "^3.9.1",
    "gulp-angular-templatecache": "^1.7.0",
    "gulp-concat": "^2.6.0",
    "gulp-eslint": "^4.0.0",
    "gulp-load-plugins": "^1.0.0-rc.1",
    "gulp-ng-annotate": "^1.0.0",
    "gulp-uglify": "^1.2.0",
    "run-sequence": "^1.1.1"
  }
